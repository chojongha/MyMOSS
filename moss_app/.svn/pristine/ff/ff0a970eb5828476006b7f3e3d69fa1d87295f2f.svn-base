<link rel="stylesheet" href="../jquery.mobile/jquery.mobile-1.3.2.min.css" />
<script src="../jquery.mobile/jquery-1.7.2.min"></script>
<script src="../jquery.mobile/jquery.mobile-1.3.2.min.js"></script>

<script type="text/javascript" charset="utf-8" src="../js/src/cordova.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/src/gibberish-aes.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/src/jquery.form.js"></script>
<script type="text/javascript" src="../js/src/webintent.js"></script>
<script type="text/javascript" src="../js/src/iscroll.js"></script>
		
<script type="text/javascript" charset="utf-8" src="../js/common.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/login.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/config.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/alarmList_noti.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/alarmList_noti_detail.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/alarmList_TFMS.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/alarmList_TFMS_detail.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/board_manual.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/board_manual_detail.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/transOffice.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/transOffice_detail.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/board_notice.js"></script>
<script type="text/javascript" charset="utf-8" src="../js/board_notice_detail.js"></script>
