</head>

<body onload="init();">
	<div data-role="page" id="page_login">
		<div data-role="header" data-position="fixed"></div>
			<div data-role="content" id="content">
				<div id="main_logo01" align="center" style="margin-top:4em;">
				<img alt="KT MOSS" src="../img/mainimg01.png" width="80%">
				</div>
				<div class="box login">
					<fieldset class="boxBody">
						<div style="margin:1em;">
							<center><input type="text" id="login_id" placeholder="사번" data-role="none">	</center>
							<center><input type="password" id="login_pw" placeholder="비밀번호" data-role="none"></center>
						</div>
					</fieldset>
					<footer>
						<fieldset class="ui-grid-solo" style="padding:0.5em 1em 0 0.5em;">
						<div style="float:right">
							<div style="display:inline-block;">자동로그인</div>
							<div style="display:inline-block;vertical-align:middle;">
							<select name="flip-7" id="login_auto" data-role="slider" data-mini="true">
							<option value="N">Off</option>
							<option value="Y">On</option>
							</select>
							</div>
						</div>
						</fieldset>
						<fieldset class="ui-grid-a" style="padding:0.5em;">
							<div class="ui-block-a"><button id="btn_submit" data-theme="c" data-corners="false" data-mini="true">확인</button></div>
							<div class="ui-block-b"><button id="btn_cancel" data-theme="c" data-corners="false" data-mini="true">취소</button></div>
						</fieldset>
					</footer>
				</div>
				<footer id="main">
					<div id="main_logo02" align="center" style="margin-bottom: 2em;">
						<img alt="O" src="../img/mainimg06.png" width="40em">
					</div>
					<p>Copyright(c) 2014 kt corp. all rights reserved.</p>
				</footer>
			</div>
		<div data-role="footer" data-position="fixed"></div>
	</div>
</body>
</html>
