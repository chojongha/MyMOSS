function checkRadioButton(objName){
	var radioObj=document.all(objName);
	var isChecked;
	
	for(i=0; i<radioObj.length; i++){
		if(radioObj[i].checked){
			isChecked=radioObj[i].value;
			break;
		}
	}
	if(isChecked=='고객전송'){
		$(location).attr('href','board_manual.html?checked=customerSend');
	}else if(isChecked=="기간전송"){
		$(location).attr('href','board_manual.html?checked=dateSend');
	}else if(isChecked=="인터넷"){
		$(location).attr('href','board_manual.html?checked=internet');
	}else if(isChecked=="전원"){
		$(location).attr('href','board_manual.html?checked=switch');
	}else if(isChecked=="교환"){
		$(location).attr('href','board_manual.html?checked=change');
	}else if(isChecked=="초고속"){
		$(location).attr('href','board_manual.html?checked=speed');
	}else{
		$(location).attr('href','board_manual.html?checked=customerNet');
	}
}

var getParam = function(key){
    var _parammap = {};
    document.location.search.replace(/\??(?:([^=]+)=([^&]*)&?)/g, function () {
        function decode(s) {
            return decodeURIComponent(s.split("+").join(" "));
        }

        _parammap[decode(arguments[1])] = decode(arguments[2]);
    });

    return _parammap[key];
};

