
function showManualDetail(){
	
	$.ajax({
		url: url+"WorkOdrList",		
		type: "post",
		timeout : 10 * 1000,
		dataType : "json",
		data: {workerID:'91038948'}, //인풋(접속)
		
		success: function (result) {
			var bodyData = result.body; //아웃풋 정보 가져오기
			
			$.each(bodyData, function(key, val){ //아웃풋 뿌리기
				
				var manualDetailTB = '<table>'+
									 '<tr>'+
									 '<th class="th" width="30%">제 목</th>'+
									 '<td colspan="3" class="td"  id="manual_title"></td>'+
									 '</tr>'+
									 '<tr>'+
									 '<th class="th" width="30%">작 성 자</th>'+
									 '<td colspan="3" class="td" id="manual_writer"><a href="transOffice_detail.html">눌러봐</a></td>'+
									 '</tr>'+
									 '<tr>'+
									 '<th class="th" width="30%">작성일시</th>'+
									 '<td colspan="3" class="td"  id="manual_writeDate"></td>'+
									 '</tr>'+
									 '</table>';
																				 				
				$('#table_manualDetail').html(manualDetailTB);
								
				$('#manual_title').text(val.odrType);
				$('#manual_writer').append(val.custName);
				$('#manual_writeDate').text(val.rsvDT);
				$('#manual_content').append(val.addr);
				$('#manual_add').text(val.custName);			
		    });
		},
		error: function(jqXHR, textStatus, errorThrown) {
	        console.log(jqXHR.responseText);
	    }
	}); 
}	
	
$(document).on("pagebeforeshow","#page_manual_detail",function(event){
	showManualDetail();
}); 
