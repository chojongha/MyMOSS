						<tr>
							<td class="td_subject" align="left" width="40%">담당(정)</td>
							<td class="td" align="left" id="TO_charge"></td>
						</tr>
						<tr>
							<td class="td_subject" align="left" width="40%">연락처(정)</td>
							<td class="td" align="left" id="TO_chargeTel"></td>
						</tr>
						<tr>
							<td class="td_subject" align="left" width="40%">담당(부)</td>
							<td class="td" align="left" id="TO_subCharge"></td>
						</tr>
						<tr>
							<td class="td_subject" align="left" width="40%">연락처(부)</td>
