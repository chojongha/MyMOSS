//라디오버튼 생성
$(document).on("pagebeforeshow","#page_manualList", function(event){
	
	$.ajax({		
		url: url+"WorkOdrList",
		type: "post",
		timeout: 10*1000,
		dataType: "json",
		data: {workerID:'91038948'},
		success: function(result){
			var bodyData=result.body;
			var manualPop="";
			
			if(bodyData.length > 0){
				$.each(bodyData, function(key,val){
					manualPop += "<input type='radio' name='manual' data-theme='d' data-iconpos='right' id='" + val.custName + "' value='" + val.custName + "' checked=''>" +
								 "<label for='" + val.custName + "'>"+val.custName+"</label>\n";
				});
			}else{
				manualPop = '<h2>없음</h2>';
			}
			
			$('#manual-controlgroup').html(manualPop).trigger("create");
		},
		error: function(jqXHR, textStatus, errorThrown){
			
		}
	});
});

