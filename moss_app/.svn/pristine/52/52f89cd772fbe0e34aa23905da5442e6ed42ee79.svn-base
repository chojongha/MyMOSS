@CHARSET "EUC-KR";

/* content viewer */
#content {
	padding:0;
}
/* list view */
#listview {
	margin:0;
}
/* 테이블 글자 크기 및 정렬 */
table {
	border:1px solid #a1a1a1;
	border-collapse: collapse;
	width: 100%;
	font-size:14px;
}
tr, td, th {
	border:1px solid #a1a1a1;
	border-collapse: collapse;
}
.td {
	padding-left:7px;
}
.th {
	background-color:#b0c4de;
}