}
#transOffice_content .ui-grid-a .ui-block-a {
	width: 70%;	
}
#transOffice_content .ui-grid-a .ui-block-b {
	width: 30%;	
}

#workplacegroup {
	margin: 5px 5px 0px 5px;	/*위>오른>아래>왼쪽*/
}
