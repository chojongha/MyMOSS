div.test
{
white-space:nowrap; 
width:25em;
overflow:hidden; 
border:1px solid #000000;
}
#contents {
	padding:0;
}
#manual_content{
	font-size:1em;
}

/* 상세 페이지 확대용 */
#contentWrapper {
	position:absolute; z-index:1;
	top:0; bottom:0; left:3px; right:3px;
	padding-right:3px;
	width:auto;
	overflow:auto;
}

.div-table{
  display:table;         
  width:auto;         
  background-color:#eee;         
  border:1px solid  #666666;         
  border-spacing:5px;/*cellspacing:poor IE support for  this*/
}
.div-table-row{
  display:table-row;
  width:auto;
  clear:both;
}
.div-table-col{
  float:left;/*fix for  buggy browsers*/
  display:table-column;         
  width:200px;         
  background-color:#ccc;  
}
/* 상세화면 테이블 style */
table.basic { 
width:100%; 
line-height:21px; 
border-top: 3px solid #cccccc; 
border-left: 3px solid #cccccc; 
border-collapse: collapse;

} 
table.basic th, table.basic td { 
color:#678197; 
text-align:left; 
border-top: 1px solid #cccccc; 
border-left: 1px solid #cccccc; 
border-right: 1px solid #cccccc; 
border-bottom: 1px solid #cccccc; 
padding: 0.5em;
white-space:pre-wrap;
} 
table.basic th { 
background-color: #eeeeee; 
}
/* 고장상황 상세 페이지 하단 navbar style */
#detail_navbar img {
	width: 2em;
	padding: 0;
	margin: 0;
}
#detail_navbar p {
	text-align: center;
	font-size: 0.7em;
	padding: 0;
	margin: 0;
}
#detail_navbar ul{
	padding: 0;
	margin-bottom: -0.5em;
}

/* Menu Panel Style */
#menuPanel_header img {
	width: 2em;
}
#menuPanel_header p {
	margin: 0;
	padding: 0;
	font-size: 0.6em;
}
#menuPanel_collapsible div {
	display:inline-block;
	vertical-align:middle;
}
#menuPanel_collapsible img {
	width: 1.5em;
	vertical-align: middle;
}
#menuPanel_collapsible p {
	font-size: 1em;
	font-weight: 600;
	margin: 0;
	padding: 0;
}
#menuItemPadding a {
	padding-left: 0.6em;
	padding-top: 0.4em;
	padding-right: 0.2em;
	padding-bottom: 0.4em;
}

/* 업무매뉴얼, 공지사항, 연락처 조회 리스트 Style */
.listContent h2 {
	font-size: 1.1em;
	margin: 0;
	padding: 0;
}
.listContent p {
	font-size: 1em;
	margin: 0;
	padding: 0;
}
/* 고장상황 진행/완료 제목 사이즈 */
.listSubject {
	font-size:1.1em;
}
.listImg_tt {
	height:1em; 
	width:1em;
	margin-right: 0.1em; 
	margin-bottom: -0.1em;
}

/*업무매뉴얼 셀렉트박스와 검색창*/
