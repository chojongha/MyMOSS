	width: 30%;
}
#manual_content	.ui-grid-b .ui-block-b {
	width: 50%;
}
#manual_content	.ui-grid-b .ui-block-c {
	width: 20%;
}
#listManual{
	margin: 0px;
	padding: 0px;
}
