@charset "euc-kr";
body { 
	margin: 0; 
	padding : 0;
	font-family: dotum; font-size: 12px;
}
img {border:0;}

ul{list-style: none;}

#wrap { 
	margin:0 auto; 
	width:100%;
}

#main_logo01{
	text-align:center;
	vertical-align:middle;
	line-height:14px;
	height:66px;
	margin:1em 0em 2em 0;
}
#main_logo02{
	text-align:center;
}
#container{
	margin:0 auto;
	position:relative;
	text-align:center;
	width:100%;
}
#intro{
 margin:0 auto;
 height:800px;
 display:table;
 
 vertical-align:middle;
 padding-top:70px;
 text-align:center;
}
#intro p{
 margin:50px 0;
}
#mainmenu{ 
	margin:0 auto;
	display:table;
	text-align:center;

}
#mainmenu ul {	
	display:table; width:20em; margin:0; padding:0; list-style:none;
}
#mainmenu ul li {
	float:left;
	width: 8em; 
	height:11em; 
	margin:1em;
	/*margin:0px 1em 2em 3em;*/
}

#mainmenu ul li img
{
	width:100%;
}
#footer{
	width:100%;
	height:35px;
}

#login{
 margin:0 auto;
 padding:0;
 width:auto;
 height:auto;
 display:table;
 vertical-align:middle;
 text-align:center;
}
#login p{
	font-weight:bold;
	color:#000;
	font-size:40px;
}
#login_box{
 border-top:1px solid #d8d8d8;
 border-right:2px solid #d8d8d8;
 border-bottom:2px solid #d8d8d8;
 border-left:1px solid #d8d8d8;
 width:100%;
 height:auto;
 margin:0 auto;
 background:url(images/login_bg.gif);
 
}
#login_box ul{
	text-valign:center;
	padding:0;
	width:100%;
}
#login_box li{
	text-valign:center;
	vertical-align:middle;
	font-size:16px;
	font-weight:bold;
	margin:0em 1em 0em 1em;
}
#login_box li input{
	text-valign:center;
	vertical-align:middle;
	border:1px solid #b2b2b2;
}

/* content viewer */
#content {
	padding:0.5em;
}
/* list view */
#listview {
	margin:0;
}
#dynamicList {
	font-size:0.7em; 
	border:0;
	width:100%;
}

.dynamicTable_td {
	border:1px;
	border-collapse: collapse;
	width:100px; 
	text-overflow:ellipsis; 
}
